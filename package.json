{
	"name": "catboy-nexus-tabletop",
	"type": "module",
	"engines": {
		"node": "20"
	},
	"scripts": {
		"dev": "run-p --race dev:*",
		"dev:remix": "remix dev",
		"dev:convex": "convex dev",
		"build": "remix build",
		"start": "remix-serve ./build/server/index.js",
		"lint": "biome check .",
		"format": "run-s --continue-on-error biome-apply prettier-write",
		"format-unsafe": "run-s --continue-on-error biome-apply-unsafe prettier-write",
		"prettier-write": "prettier . --write",
		"biome-apply": "biome check . --apply",
		"biome-apply-unsafe": "biome check . --apply-unsafe"
	},
	"dependencies": {
		"@ariakit/react": "0.4.0",
		"@clerk/remix": "3.1.16",
		"@clerk/themes": "1.7.9",
		"@fontsource/barlow": "5.0.8",
		"@radix-ui/react-scroll-area": "1.0.5",
		"@remix-run/node": "2.5.1",
		"@remix-run/react": "2.5.1",
		"@remix-run/serve": "2.5.1",
		"@tw-classed/react": "1.7.0",
		"date-fns": "3.3.1",
		"isbot": "^4",
		"lucide-react": "0.314.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-virtuoso": "4.6.2",
		"tailwind-merge": "2.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@remix-run/dev": "2.5.1",
		"@tailwindcss/container-queries": "0.1.1",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"@vercel/remix": "2.5.1",
		"@vitejs/plugin-react-swc": "3.5.0",
		"autoprefixer": "10.4.17",
		"convex": "1.8.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.33",
		"prettier": "3.2.4",
		"prettier-plugin-jsdoc": "1.3.0",
		"prettier-plugin-tailwindcss": "0.5.11",
		"tailwindcss": "3.4.1",
		"tailwindcss-animate": "1.0.7",
		"typescript": "5.3.3",
		"vite": "5.0.12",
		"vite-tsconfig-paths": "4.3.1"
	}
}
